{% extends "admin/base.html.twig" %}

{% block title %}
    {% if form.vars.value.codedomaine is null %}
        Ajouter un domaine d'activit√©
    {% else %}
        Modifier le domaine {{ form.vars.value.nomdomaine }}
    {% endif %}
{% endblock %}

{% block body %}
    <div class="box">
        <div class="box-body">
            {{ form_start(form) }}

            {{ form_row(form.nomdomaine) }}

            <button type="submit" class="btn btn-primary btn-block" >Envoyer</button>

            {{ form_end(form) }}
        </div>
    </div>
{% endblock %}