<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Etudiant" table="etudiant" repository-class="AppBundle\Repository\EtudiantRepository" >
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="nomEtudiant" type="string" column="nomEtudiant" length="255">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="prenomEtudiant" type="string" column="prenomEtudiant" length="255">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="mailEtudiant" type="string" column="mailEtudiant" length="255">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="telEtudiant" type="string" column="telEtudiant" length="10">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <one-to-one field="codeProposition" target-entity="Propositions" mapped-by="codeEtudiant" />
      <many-to-one field="codeclasse" target-entity="Classes" fetch="LAZY">
          <join-columns>
              <join-column name="codeClasse" referenced-column-name="codeClasse" nullable="false"/>
          </join-columns>
      </many-to-one>
  </entity>
</doctrine-mapping>
